<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SAME_YEAR</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT(ISBLANK(SLAExpirationDate__c)),
NOT(YEAR(TODAY())= YEAR(SLAExpirationDate__c)))</errorConditionFormula>
    <errorMessage>SAME YEAR</errorMessage>
</ValidationRule>
