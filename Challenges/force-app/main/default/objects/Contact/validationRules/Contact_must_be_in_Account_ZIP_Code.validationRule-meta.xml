<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contact_must_be_in_Account_ZIP_Code</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT(ISBLANK(AccountId)),
 Account.ShippingPostalCode &lt;&gt;  MailingPostalCode )</errorConditionFormula>
    <errorMessage>Should match parentaccount shipping postal code.</errorMessage>
</ValidationRule>
